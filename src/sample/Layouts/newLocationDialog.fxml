<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<DialogPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="sample.NewLocationController"
            prefHeight="100" prefWidth="500">
    <content>
        <GridPane hgap="10">
            <Label text="Enter the city you want to add " GridPane.rowIndex="0" GridPane.columnIndex="0"/>
            <TextField fx:id="cityName" GridPane.columnIndex="1" GridPane.rowIndex="0"/>

        </GridPane>
    </content>

</DialogPane>
